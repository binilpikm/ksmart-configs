{
  "key": "tlcertificate",
  "config": {
      "defaultStyle": {
          "font": "Thumba"
           },
          "content": [
            {
              "style": "ws-head",
              "table": {
                "widths": ["100%"],
                "body": [
                  [
                    {
                      "stack": [
                        {
                          "text": "{{localbodyName}}",
                          "style": "appendix-header-italic"
                        },
                        {
                          "text": "Issued under Section 447 of the Kerala Municipality Act 1994 (20th Act of 1994)",
                          "style": "main-body-style"
                        },
                        {
                          "text": "IFTE & OS LICENCE",
                          "style": "appendix-header-body"
                        }
                      ]
                    },
                    
                    {
                      "style": "ws-head",
                      "table": {
                        "widths": ["*"],
                        "body": [
                          [
                            {
                              "columns": [
                                {
                                  "text": "License No",
                                  "width": "35%",
                                  "style": "details-body"
                                },
                                {
                                  "text": ": ",
                                  "width": "1%"
                                },
                                {
                                  "text": "{{licenseNumber}}",
                                  "style": "details-body"
                                }
                              ]
                            }
                          ],
                          [
                            {
                              "columns": [
                            
                                {
                                  "text": "Licensing Unit",
                                  "width": "35%",
                                  "style": "details-body"
                                },
                                {
                                    "text": ": ",
                                    "width": "1%"
                                },
                                {
                                  "text": "{{licenseUnitName}}",
                                  "style": "details-body"
                                }
                              ]
                            }
                          ]
                        ]
                      },
                      "layout": "noBorders"
                    }
                  ]
                ]
              },
              "layout": "noBorders"
            }
          ],
           "styles": {
            "main-body-style": {
              "color": "#000000",
              "fontSize": 8,
              "alignment": "center"
               }
          },
          "appendix-header-italic": {
            "color": "#000000",
            "fontSize": 9,
            "fontWeight": 400,
            "alignment": "center"
          },
          "appendix-header-body": {
            "color": "#000000",
            "bold": true,
            "fontSize": 9,
            "fontWeight": 400,
            "alignment": "center"
          },
          "details-body": {
            "color": "#000000",
            "bold": false,
            "fontSize": 10,
            "fontWeight": 400,
            "margin": [0, 5, 0, 0]
          }
        }
      }
